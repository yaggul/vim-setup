execute pathogen#infect()
let g:jedi#setup_py_version = 3

colorscheme evening
set ruler
set showcmd
set wildmenu
syntax on

set nohlsearch
set incsearch

set ignorecase
set smartcase

set backspace=indent,eol,start

set autoindent

set laststatus=2

set confirm

set cmdheight=2

set number
set nocompatible
"filetype off


set rtp+=~/.vim/bundle/Vundle.vim

call vundle#begin()


call vundle#end()

filetype plugin indent on


